import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc = new Scanner(System.in);
        
        int num_1 = sc.nextInt();
        int num_2 = sc.nextInt();
        char ch = sc.next().charAt(0);
        
        switch(ch){
            case '+':
                System.out.printf("Addition of two number is %.1f",(float)(num_1 + num_2));
                break;
                
                case '-':
                System.out.printf("Subtraction of two number is %.1f",(float)(num_1 - num_2));
                break;
                
                case '*':
                System.out.printf("Multiplication of two number is %.1f",(float)(num_1 * num_2));
                break;
                
                case '/':
                System.out.printf("Division of two number is %.1f",(float)(num_1 / num_2));
                break;
                
                case '%':
                System.out.printf("Modulo of two number is %.1f",(float)(num_1 % num_2));
                break;
                
            default:
                System.out.println("Invalid Input");
        }
    }
}
